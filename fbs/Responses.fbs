namespace GoPServer.protocol;
table CreateResponse {
  error: string;
}

table PullResponse {
  offsets: [uint];
  buffer: [double];
  id:byte;
}

union ResponsePayLoad {
  CreateResponse,
  PullResponse
}

table ParameterServerResponse {
  name:string;
  payload:ResponsePayLoad;
}

table Responses {
  res: [ParameterServerResponse];
}

root_type Responses;